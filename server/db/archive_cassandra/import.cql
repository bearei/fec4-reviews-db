CREATE TABLE bearei.reviews (rating tinyint, title varchar, text varchar, recommend boolean, name varchar, fit tinyint, "itemId" int, helpful tinyint, "notHelpful" tinyint, flag boolean, "createdAt" date, TimeUUID PRIMARY KEY);

